<div class="jumbotron descr-top-sobre">
    <div class="container text-center">
        <h1>{{contatoinfo.titulo}}</h1>
        <p class="lead mb-4">{{contatoinfo.subtitulo}}</p>
    </div>
</div>

<div class="jumbotron contato">
    <div class="container">
        <div class="row featurette">
            <div class="col-md-6">
                <h3 class="featurette-heading exp-home-titulo">{{contatoinfo.tituloform}}</h3>
                <form>
                    <div class="form-group">
                        <label>Nome</label>
                        <input type="text" class="form-control" placeholder="Nome completo">
                    </div>
                    <div class="form-group">
                        <label>E-mail</label>
                        <input type="email" class="form-control" placeholder="Seu Melhor E-mail">
                    </div>
                    <div class="form-group">
                        <label>Assunto</label>
                        <input type="text" class="form-control" placeholder="Assunto da mensagem">
                    </div>
                    <div class="form-group">
                        <label>Mensagem</label>
                        <textarea class="form-control" rows="6"></textarea>
                    </div>
                    <button type="submit" class="btn btn-danger">Enviar</button>
                </form>
            </div>
            <div class="col-md-6">
                <h3 class="featurette-heading exp-home-titulo">{{contatoinfo.titulohratend}}</h3>
                <p class="lead">{{contatoinfo.hratent}}</p>
                <hr>
                <address>
                    <strong>{{contatoinfo.tituloend}}.</strong><br>
                    {{contatoinfo.logradouro}}<br>
                    {{contatoinfo.bairro}} - {{contatoinfo.complemento}}<br>
                    {{contatoinfo.telefone}}
                </address>
            </div>
        </div>
    </div>
</div>

<div class="jumbotron contato">
    <div class="container">

    </div>
</div>

{{>_rodape}}